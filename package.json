    "test": "cross-env NODE_ENV=testing jest --verbose --runInBand"
  },
  "dependencies": {
    "@types/jest": "^30.0.0",
    "cross-env": "^10.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jest": "^30.0.5",
    "knex": "^2.4.2",
    "sqlite3": "^5.1.4",
    "supertest": "^7.1.4"
  },
  "devDependencies": {
    "eslint": "^8.36.0",
    "nodemon": "^3.1.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bloominstituteoftechnology/node-testing2-guided.git"
  }
}
