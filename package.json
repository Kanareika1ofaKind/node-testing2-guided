{
  "name": "node-testing2-guided",
  "version": "1.0.0",
  "scripts": {
    "server": "nodemon index.js",
    "start": "node index.js",
    "rollback": "knex migrate:rollback",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "ketchup": "git fetch && git reset --hard origin/lecture",
